            "microbit-dal": {
                "bluetooth": {
                    "private_addressing": 0,
                    "advertising_timeout": 0,
                    "tx_power": 0,
                    "dfu_service": 0,
                    "event_service": 0,
                    "device_info_service": 0,
                    "eddystone_url": 0,
                    "eddystone_uid": 0,
                    "open": 0,
                    "pairing_mode": 0,
                    "whitelist": 1,
                    "security_level": "SECURITY_MODE_ENCRYPTION_NO_MITM",
                    "partial_flashing": 1,
                    "enabled": 0
                }
            }
        }
    },
    "panic_on_heap_full": 0,
    "authors": [
        "Luqmaan Baboo",
        "Bjarke Gotfredsen"
    ]
