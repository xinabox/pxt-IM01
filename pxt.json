{
    "name": "IM01",
    "version": "0.1.1",
    "description": "XinaBox new IM01 extension",
    "license": "MIT",
    "dependencies": {
        "core": "*"
    },
    "files": [
        "README.md",
        "sdmm.cpp",
        "IM01.ts",
        "cs11.cpp",
        "diskio.h",
        "ff.cpp",
        "ff.h",
        "ffconf.h"
    ],
    "testFiles": [
        "test.ts"
    ],
    "public": true,
    "supportedTargets": [
        "maker",
        "microbit"
    ],
    "preferredEditor": "tsprj",
